{
  "defaultSeverity": "warning",
  "extends": ["tslint:recommended"],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "quotemark": [true, "single"],
    "member-access": [false],
    "max-line-length": [true, 150],
    "interface-name": [false],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
