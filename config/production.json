{
  "logger": {
    "source": false,
    "console": {
      "enable": true,
      "level": "debug"
    },
    "file": {
      "enable": false,
      "level": "info",
      "dir": "./logs",
      "addHostnameToPath": false
    },
    "server": {
      "enable": true,
      "level": "trace",
      "url": "x.x.x.x",
      "port": "9998",
      "type": "elk",
      "X-OVH-TOKEN": "xxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
    },
    "ringBuffer": {
      "enable": true,
      "size": 50
    }
  }
}
  